{
  "name": "public",
  "version": "0.0.6",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/qwertyyb/public.git"
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=development electron dist/index.js --remote-debugging-port=8222",
    "start:electron": "nodemon --exec 'npm run electron || exit 1'",
    "start:render": "cd ./render2 && npm run start",
    "webpack:main": "webpack --config ./webpack/webpack.config.main.ts -w",
    "webpack:preload": "webpack --config ./webpack/webpack.config.preload.ts -w",
    "webpack:plugin": "webpack --config ./webpack/webpack.config.plugin.ts -w",
    "webpack:plugin:preload": "webpack --config ./webpack/webpack.config.plugin.preload.ts -w",
    "start": "concurrently --names \"main,preload,plugin,render,electron\" -c \"bgBlue.bold,bgBlack.bold,bgMagenta.bold,bgYellow.bold,bgGreen.bold\" \"npm run webpack:main\" \"npm run webpack:preload\" \"npm run webpack:plugin\" \"npm run start:render\" \"wait-on tcp:localhost:3000 && npm run start:electron\"",
    "build:main": "webpack --config ./webpack/webpack.config.main.ts --mode=production",
    "build:preload": "webpack --config ./webpack/webpack.config.preload.ts --mode=production",
    "build:plugin": "webpack --config ./webpack/webpack.config.plugin.ts --mode=production",
    "build:plugin:preload": "webpack --config ./webpack/webpack.config.plugin.preload.ts --mode=production",
    "build:render": "cd ./render2 && npm run build",
    "build": "concurrently --names \"main,preload,plugin,render\" -c \"bgBlue.bold,bgGreen.bold,bgMagenta.bold,bgYellow.bold\" \"npm run build:main\" \"npm run build:preload\" \"npm run build:plugin\" \"npm run build:render\"",
    "pack": "electron-builder -c.mac.identity=null --dir",
    "release": "NODE_ENV=production && npm run build:render && npm run tsc && electron-builder --publish=always"
  },
  "nodemonConfig": {
    "watch": [
      "dist/**/*"
    ]
  },
  "keywords": [],
  "author": "qwertyyb",
  "description": "public tools",
  "license": "ISC",
  "build": {
    "appId": "cn.qwertyyb.public",
    "asar": false,
    "files": [
      "dist/**/*",
      "assets/**/*",
      "!**/node_modules",
      "plugins/*/package.json",
      "plugins/*/dist/**/*",
      "plugins/*/lib/**/*",
      "plugins/*/public/**/*",
      "!plugins/src",
      "render2/dist/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "build"
    },
    "afterSign": "scripts/notarize.js",
    "mac": {
      "asar": true,
      "category": "public.app-category.utilities",
      "type": "distribution",
      "identity": "Yongbang Yang (T68XK6867P)",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "electronLanguages": [
        "en",
        "zh_CN"
      ],
      "extendInfo": {
        "LSBackgroundOnly": 1,
        "LSUIElement": 1
      }
    }
  },
  "pnpm": {
    "overrides": {
      "node-gyp": "^10.0.0"
    },
    "patchedDependencies": {
      "@nut-tree-fork/node-mac-permissions@2.2.1": "patches/@nut-tree-fork__node-mac-permissions@2.2.1.patch",
      "@nut-tree-fork/libnut-darwin@2.7.2": "patches/@nut-tree-fork__libnut-darwin@2.7.2.patch",
      "better-sqlite3@11.1.1": "patches/better-sqlite3@11.1.1.patch"
    }
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@electron/remote": "^2.1.2",
    "@nut-tree-fork/nut-js": "^4.2.1",
    "@types/better-sqlite3": "^7.6.10",
    "@types/mathjs": "^6.0.11",
    "@types/node": "^12.20.55",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.5",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-notarize": "^1.0.0",
    "electron-updater": "^4.3.5",
    "file-icon": "^4.0.0",
    "isbinaryfile": "^5.0.2",
    "mathjs": "^13.0.0",
    "node-gyp": "^10.0.0",
    "node-loader": "^2.0.0",
    "nodemon": "^2.0.7",
    "qrcode": "^1.4.4",
    "style-loader": "^4.0.0",
    "tiny-pinyin": "^1.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "wait-on": "^7.2.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@public/osx-fileicon": "workspace:*",
    "better-sqlite3": "^11.1.1",
    "uiohook-napi": "^1.5.4"
  }
}
